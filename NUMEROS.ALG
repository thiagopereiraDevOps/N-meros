Algoritmo "numeros"

Var

   num_inicial, num_final, i: inteiro

Inicio

     Escreva("Digite o número inicial: ")
     Leia(num_inicial)
     Escreva("Digite o número final: ")
     Leia(num_final)
     i <- num_inicial
     enquanto (i<=num_final) faca
        Escreval(i)
        i <- i+1
     fimenquanto
     Escreval("Fim!!")
Fimalgoritmo